{
    "name": "bag2/meta",
    "description": "Library for meta programming.",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^9.4"
    },
    "autoload": {
        "psr-4": {
            "Bag2\\Meta\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["src/functions.php"],
        "psr-4": {
            "Bag2\\Meta\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
